{
  "name": "ci-state-indicator-backend",
  "version": "0.1.0",
  "description": "Backend for continuous integration system state indicator (also knowns as Traffic light)",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest",
    "coverage": "jest --collect-coverage"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "./__tests__/teamcityTestUtils.js",
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "verbose": true
  }
}
